class Math {
    int facto (int n) {
        Math ma = new Math();
        int x;
        if (n <= 1) {
            return 1;
        } else {
            x = ma.facto(n-1); 
            return x * n;
        }
    }
}

{   
    Math m = new Math();

    int a = m.facto(5);
    
    println(a);
}